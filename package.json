{
  "name": "stalker-to-m3u",
  "version": "1.1.0",
  "scripts": {
    "pregroups": "tsc --sourceMap --strict types.ts common.ts",
    "groups": "ts-node --project ./tsconfig.ts-node.json generate-groups.ts",
    "prem3u": "npm run pregroups",
    "m3u": "ts-node --project ./tsconfig.ts-node.json generate-m3u.ts",
    "m3u-iptv": "npm run groups iptv && npm run m3u iptv",
    "m3u-vod": "npm run groups vod && npm run m3u vod",
    "m3u-series": "npm run groups series && npm run m3u series",
    "preiptv-analyzer": "npm run pregroups",
    "iptv-analyzer": "ts-node --project ./tsconfig.ts-node.json tools/iptv-anayzer.ts"
  },
  "private": false,
  "devDependencies": {
    "@types/node": "^12.11.1",
    "ajv": "latest",
    "axios": "latest",
    "chalk": "^4.1.2",
    "fs": "latest",
    "follow-redirects": "latest",
    "http": "latest",
    "iswitch": "latest",
    "rxjs": "^7.8.1",
    "ts-node": "10.1.0",
    "typescript": "~4.3.2",
    "yargs-parser": "latest"
  }
}
