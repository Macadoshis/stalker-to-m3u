{
  "name": "stalker-to-m3u",
  "version": "1.5.0",
  "scripts": {
    "compile": "tsc --noEmit --pretty false",
    "pregroups": "tsc --sourceMap --strict --target ES2021 --module commonjs types.ts common.ts",
    "groups": "ts-node --project ./tsconfig.ts-node.json generate-groups.ts",
    "prem3u": "npm run pregroups",
    "m3u": "ts-node --project ./tsconfig.ts-node.json generate-m3u.ts",
    "m3u-iptv": "npm run groups iptv && npm run m3u iptv",
    "m3u-vod": "npm run groups vod && npm run m3u vod",
    "m3u-series": "npm run groups series && npm run m3u series",
    "preiptv-analyzer": "npm run pregroups",
    "iptv-analyzer": "ts-node --project ./tsconfig.ts-node.json tools/iptv-analyzer.ts",
    "prem3u-tester": "npm run pregroups",
    "m3u-tester": "ts-node --project ./tsconfig.ts-node.json tools/m3u-tester.ts",
    "preiptv-generator": "npm run pregroups",
    "iptv-generator": "ts-node --project ./tsconfig.ts-node.json tools/iptv-generator.ts"
  },
  "private": false,
  "devDependencies": {
    "@google/genai": "~1.4.0",
    "@types/node": "^24.0.7",
    "@types/object-hash": "latest",
    "ajv": "latest",
    "async-mutex": "latest",
    "axios": "latest",
    "axios-retry": "latest",
    "chalk": "^4.1.2",
    "fs": "latest",
    "fluent-ffmpeg": "latest",
    "http": "latest",
    "iptv-playlist-parser": "latest",
    "iswitch": "latest",
    "object-hash": "latest",
    "rxjs": "^7.8.1",
    "ts-node": "10.1.0",
    "typescript": "~5.8.2",
    "yargs-parser": "latest"
  }
}
